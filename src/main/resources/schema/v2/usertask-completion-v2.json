{
    "$schema": "https://json-schema.org/draft/2020-12/schema",
    "$id": "https://example.com/usertask.completion-v2.json",
    "title": "Usertask Completion V2",
    "description": "A completion of an unsertask",
    "type": "object",
    "properties": {
        "userId": {
            "description": "The unique identifier for an user",
            "type": "integer"
        },
        "name": {
            "description": "Name of the user",
            "type": "string"
        },
        "age": {
            "description": "Age of the user",
            "type": "integer"
        },
        "hobbies": {
            "type": "array",
            "items": {
                "type": "string"
            }
        }
    },
    "required": [
        "userId",
        "name",
        "age"
    ]
}